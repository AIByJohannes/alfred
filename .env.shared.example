# Shared Environment Variables for Alfred Platform
# Copy these to individual service .env files

# Security (MUST BE IDENTICAL ACROSS SERVICES)
# Algorithm: HS256
JWT_SECRET=your-256-bit-secret-here

# Database Configuration
DB_HOST=postgres
DB_PORT=5432
DB_NAME=alfred
DB_USER=user
DB_PASSWORD=pass

# Service URLs
NEXT_PUBLIC_API_URL=http://localhost:8080 # Spring Boot
NEXT_PUBLIC_AI_URL=http://localhost:8000  # FastAPI
